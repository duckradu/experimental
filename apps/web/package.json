{
  "name": "@experimental/web",
  "type": "module",
  "version": "0.0.0",
  "author": "radu.dascalu@pm.me",
  "scripts": {
    "pre-commit": "pnpm generate:schema-barrel",
    "dev": "vinxi dev",
    "build": "vinxi build",
    "build-and-migrate": "pnpm build && pnpm db:migrate",
    "build-and-migrate-and-seed": "pnpm build && pnpm db:migrate && pnpm db:seed",
    "start": "vinxi start",
    "version": "vinxi version",
    "db:check": "drizzle-kit check --config=src/config/db.ts",
    "db:generate": "drizzle-kit generate --config=src/config/db.ts",
    "db:migrate": "touch .env && node --import tsx --env-file .env ./src/lib/db/migrate.ts",
    "db:push": "drizzle-kit push --config=src/config/db.ts",
    "db:seed": "touch .env && node --import tsx --env-file .env ./src/lib/db/seed.ts",
    "db:drop": "drizzle-kit drop --config=src/config/db.ts",
    "db:studio": "drizzle-kit studio --config=src/config/db.ts",
    "generate:schema-barrel": "touch .env && node --import tsx --env-file .env ./scripts/generate-db-schema-barrel.ts"
  },
  "dependencies": {
    "@solid-primitives/context": "^0.2.3",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.1",
    "@solidjs/start": "^1.0.4",
    "@unocss/reset": "^0.61.4",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "drizzle-orm": "^0.32.0",
    "nanoid": "^5.0.7",
    "postgres": "^3.4.4",
    "solid-js": "^1.8.18",
    "tailwind-variants": "^0.2.1",
    "valibot": "^0.36.0",
    "vinxi": "^0.3.14"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.228",
    "drizzle-kit": "^0.23.0",
    "glob": "^11.0.0",
    "prettier": "^3.3.3",
    "solid-devtools": "^0.30.1",
    "tsx": "^4.16.2",
    "unocss": "^0.61.4",
    "unocss-preset-animations": "^1.1.0",
    "unplugin-icons": "^0.19.0"
  }
}
